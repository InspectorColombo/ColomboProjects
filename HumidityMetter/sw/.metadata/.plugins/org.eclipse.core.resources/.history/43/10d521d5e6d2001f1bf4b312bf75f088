uint8_t* Font8x8::GetCharBitmap(const uint8_t ch) const
{
	return &fontTable8x8[0] + ((uint16_t)ch) * 8;
}



uint8_t Font8x8::GetWidth() const
{
	return 8;
}
uint8_t Font8x8::GetHeight() const
{
	return 8;
}




}	// namespace font



